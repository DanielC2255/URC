<launch>
  <node pkg="imu_filter_madgwick" type="imu_filter_node" name="imu_filter_node" output="screen">
    <param name="gain" value="0.1" />
    <param name="use_mag" value="true" />
    <param name="fixed_frame" value="imu_link" />
    <param name="publish_debug_topics" value="true" />
    <!-- INPUT TOPICS -->
    <remap from="imu/mag" to="imu/mag"/>
    <remap from="imu/data_raw" to="imu/data_raw"/>
    <!-- OUTPUT TOPICS -->
    <remap from="imu/data" to="imu/data_filtered"/>
  </node>
</launch>

