<?xml version="1.0"?>
<robot name ="cougarbot">
  
 <link name="base_link">
    <visual>
      <geometry>
        <box size="0.16 1.29 0.12"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.085"/>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
  </link>
  
 <link name="fl_wheel">
   <visual>
     <geometry>
        <cylinder length="0.050" radius="0.127"/>
      </geometry>
      <origin rpy="0.1 0 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
 </link>
  
  <joint name="base_to_right_leg" type="fixed">
       <parent link="base_link"/>
       <child link="fl_wheel"/>
      <origin xyz="0.22 0 .25"/>
  </joint>
  
  
</robot>
